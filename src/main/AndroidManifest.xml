<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.example.tpvcafeteria">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_cafeteria"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_cafeteria_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TPVCafeteria">
        <activity android:name=".ProductoComanda" android:screenOrientation="portrait"/>
        <activity
            android:name=".ComandasGeneralActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>